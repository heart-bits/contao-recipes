{
  "name": "heart-bits/contao-recipes-bundle",
  "description": "Recipes extension for Contao Open Source CMS",
  "keywords": [
    "recipes",
    "categories",
    "ingredients",
    "units",
    "cooking",
    "rezepte",
    "kategorien",
    "zutaten",
    "maßeinheiten",
    "kochen"
  ],
  "type": "contao-bundle",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Sascha Wustmann – Mediengestalter & Webentwickler",
      "homepage": "https://saschawustmann.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "hi@saschawustmann.com",
    "issues": "https://github.com/heart-bits/contao-recipes/issues",
    "source": "https://github.com/heart-bits/contao-recipes"
  },
  "require": {
    "php": ">=8.0",
    "contao/core-bundle": "~4.13",
    "codefog/contao-haste": "~4.25"
  },
  "require-dev": {
    "contao/manager-bundle": "~4.13"
  },
  "autoload": {
    "psr-4": {
      "Heartbits\\ContaoRecipes\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Heartbits\\ContaoRecipes\\ContaoManager\\Plugin"
  }
}
